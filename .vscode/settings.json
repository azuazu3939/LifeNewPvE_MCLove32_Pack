{
    "MythicScribe.alwaysEnabled": true,
    "MythicScribe.attributeAliasUsedInCompletions": "shorter",
    "MythicScribe.datasetSource": "Local",
    "MythicScribe.enableShortcuts": true,
    "MythicScribe.enableMythicScriptSyntax": true,
    "workbench.colorTheme": "Default Dark Modern",
    "editor.acceptSuggestionOnEnter": "on",
    "MythicScribe.fileRegex": {
        "MythicMobs": "[\\\\\\/][Mm]ythic[Mm]obs[\\\\\\/]",
        "Mob": "[\\\\\\/][Mm]obs[\\\\\\/]",
        "Item": "[\\\\\\/][Ii]tems[\\\\\\/]",
        "Metaskill": "[\\\\\\/][Ss]kills[\\\\\\/]",
        "Droptable": "[\\\\\\/][Dd]rop[Tt]ables[\\\\\\/]",
        "Stat": "[\\\\\\/]stats.((yaml)|(yml)|(txt))"
    },
    "MythicScribe.enabledPlugins": {
        "ModelEngine": false
    },
}